{
    "name": "lara-zeus/core",
    "description": "the core for zeus packages",
    "keywords": [
        "laravel",
        "lara-zeus",
        "form",
        "core",
        "contact"
    ],
    "homepage": "https://github.com/lara-zeus",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Ashraf Monshi",
            "email": "wh7r.com@gmail.com",
            "role": "Owner"
        }
    ],
    "require": {
        "php": "^8.0",
        "illuminate/support": "^8.0 || ^9.0",
        "filament/filament": "^2.10",
        "archtechx/laravel-seo": "^0.4.0",
        "calebporzio/sushi": "^2.4"
    },
    "require-dev": {
        "orchestra/testbench": "^6.0 || ^7.0",
        "phpunit/phpunit": "^9.5.10"
    },
    "autoload": {
        "psr-4": {
            "LaraZeus\\Core\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "LaraZeus\\Core\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "LaraZeus\\Core\\CoreServiceProvider"
            ],
            "aliases": {
                "Core": "LaraZeus\\Core\\CoreFacade"
            }
        }
    }
}
